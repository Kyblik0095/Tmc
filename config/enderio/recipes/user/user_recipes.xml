	<recipe name="Gear, Wood" required="true">
		<crafting>
			<grid>
				<item /><item name="stickWood"/><item />
				<item name="stickWood"/><item /><item name="stickWood"/>
				<item /><item name="stickWood"/><item />
			</grid>
			<output name="enderio:item_material:9"/>
		</crafting>
	</recipe>

	<recipe name="Gear, ae2 messes things up" disabled="true"/>
	<recipe name="Gear, just to be sure" disabled="true"/>
	<recipe name="Gear, better save than sorry" disabled="true"/>

	<recipe name="Alloy: Industrial Insulation" required="true">
		<alloying energy="5000" exp="0.5">
			<input name="dustLapis" amount="4"/>
			<input name="openblocks:sponge" amount="2"/>
			<input name="INSULATION_METAL" amount="4"/>
			<output name="INDUSTRIAL_INSULATION"/>
		</alloying>
	</recipe>

